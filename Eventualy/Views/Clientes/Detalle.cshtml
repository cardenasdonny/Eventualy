@model Eventualy.Business.Dtos.Clientes.ClienteDetalleDto
@{
    Layout = null;
}
<form>
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="form-group">
        <div class="col-md-12">
            <label asp-for="Nombres" class="control-label"></label>
            <br />
            @Model.Nombres
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-12">
            <label asp-for="TipoDocumento" class="control-label"></label>
            <br />
            @Model.TipoDocumento
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-12">
            <label asp-for="Documento" class="control-label"></label>
            <br />
            @Model.Documento
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-12">
            <label asp-for="Email" class="control-label"></label>
            <br />
            @Model.Email
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-12">
            <label asp-for="Estado" class="control-label"></label>
            <br />
            @Model.Estado
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-12">
            <label asp-for="Edad" class="control-label"></label>
            <br />
            @Model.Edad
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-12">
            <label asp-for="Cumpleanios" class="control-label"></label>
            <br />
            @Model.Cumpleanios.ToString("dd/MM")
        </div>
    </div>

</form>
<script src="~/js/site.js"></script>
@await Html.PartialAsync("_ValidationScriptsPartial")