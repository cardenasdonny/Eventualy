@model Eventualy.Model.Entities.Cliente
@{
    Layout = null;
}
<form>
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="form-group">
        <div class="col-md-12">
            <label asp-for="Nombres" class="control-label"></label>
            <br />
            @Model.Nombres
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-12">
            <label asp-for="TiposDocumento.Nombre" class="control-label"></label>
            <br />
            @Model.TiposDocumento.Nombre
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-12">
            <label asp-for="Documento" class="control-label"></label>
            <br />
            @Model.Documento
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-12">
            <label asp-for="Email" class="control-label"></label>
            <br />
            @Model.Email
        </div>
    </div>
    
</form>
<script src="~/js/site.js"></script>
@await Html.PartialAsync("_ValidationScriptsPartial")